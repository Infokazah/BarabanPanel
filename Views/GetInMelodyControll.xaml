<UserControl x:Class="BarabanPanel.Views.GetInMelodyControll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BarabanPanel.Views"
             mc:Ignorable="d" 
             xmlns:vm="clr-namespace:BarabanPanel.ViewModels"
             d:DataContext="{d:DesignInstance vm:GetInRitmViewModel, d:IsDesignTimeCreatable=True}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="0.2*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Ellipse Fill="Gray" Grid.Column="0" Grid.Row="0" />
        <Ellipse Fill="Gray" Grid.Column="0" Grid.Row="1"/>
        <Ellipse Fill="Yellow" Grid.Column="1" Grid.Row="0"/>
        <Ellipse Fill="Gray" Grid.Column="1" Grid.Row="1"/>
        
    </Grid>
</UserControl>
